{
  "interactiveLogin": true,
  "httpServer": "NettyWrapper",
  "tokenCallbacks": [
    {
      "issuerId": "maskinporten",
      "tokenExpiry": 120,
      "requestMappings": [
        {
          "requestParam": "scope",
          "match": "nav:helse/v1/helseopplysninger",
          "claims": {
            "aud": [
              "aud-localhost"
            ],
            "scope": "nav:helse/v1/helseopplysninger"
          }
        }
      ]
    },
    {
      "issuerId": "default",
      "tokenExpiry": 120,
      "requestMappings": [
        {
          "requestParam": "scope",
          "match": "eventstore",
          "claims": {
            "sub": "eventstore-subject",
            "aud": [
              "eventstore-audience"
            ]
          }
        }
      ]
    }
  ]
}
