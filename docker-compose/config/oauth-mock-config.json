{
  "interactiveLogin": true,
  "httpServer": "NettyWrapper",
  "tokenCallbacks": [
    {
      "issuerId": "maskinporten",
      "tokenExpiry": 120,
      "requestMappings": [
        {
          "requestParam": "scope",
          "match": "hops",
          "claims": {
            "sub": "helseopplysninger-subject",
            "aud": [
              "helseopplysninger-audience"
            ],
            "scope": "nav:helse/helseopplysninger.write nav:helse/helseopplysninger.read"
          }
        },
        {
          "requestParam": "scope",
          "match": "hops:pub",
          "claims": {
            "sub": "helseopplysninger-subject",
            "aud": [
              "helseopplysninger-audience"
            ],
            "scope": "nav:helse/helseopplysninger.write"
          }
        },
        {
          "requestParam": "scope",
          "match": "hops:sub",
          "claims": {
            "sub": "helseopplysninger-subject",
            "aud": [
              "helseopplysninger-audience"
            ],
            "scope": "nav:helse/helseopplysninger.read"
          }
        }
      ]
    },
    {
      "issuerId": "default",
      "tokenExpiry": 120,
      "requestMappings": [
        {
          "requestParam": "scope",
          "match": "eventstore",
          "claims": {
            "sub": "eventstore-subject",
            "aud": [
              "eventstore-audience"
            ]
          }
        }
      ]
    }
  ]
}
